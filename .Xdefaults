*font:     xft:bitstream vera sans mono:size=9:antialias=true
*boldFont: xft:bitstream vera sans mono:bold:size=9:antialias=true
*letterSpace:  0
*scrollBar:        false
*scrollBar_right:  false
*borderless:       1
*perl-ext:      default,url-select
*keysym.M-u:    perl:url-select:select_next
*url-select.launcher:   /usr/bin/firefox
*url-select.underline: true
*termName: rxvt-256color
urxvt*transparent: true
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
urxvt*shading: 50

urxvt.keysym.C-0: command:\033]710;xft:bitstream vera sans mono:size=9:antialias=true\007
urxvt.keysym.C-minus: command:\033]710;xft:bitstream vera sans mono:size=7:antialias=true\007
urxvt.keysym.C-plus: command:\033]710;xft:bitstream vera sans mono:size=11:antialias=true\007

urxvt*background:  #0f0f0f
urxvt*foreground:  #dcdccc
shoot*background:  #0f0f0f
shoot*foreground:  #dcdccc
smallTerm*background:  #0f0f0f
smallTerm*foreground:  #dcdccc
ncmpcpp*background:  #0f0f0f
ncmpcpp*foreground:  #dcdccc

