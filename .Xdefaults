!urxvt*font:             xft:Dina:pixelsize=11
!urxvt*font:             xft:Termsyn7x14r:pixelsize=11
!urxvt*font:             xft:Tamzen10x20r:pixelsize=11
urxvt*font:             xft:SourceCodePro:pixelsize=14
!urxvt*font:             xft:MonteCarloMedium:pixelsize=11
urxvt.letterSpace:  0
urxvt*scrollBar:        false
urxvt*scrollBar_right:  false
urxvt*foreground:       #8B7B8B
urxvt*background:       black
urxvt*borderless:       1
urxvt.perl-ext:      default,url-select
urxvt.keysym.M-u:    perl:url-select:select_next
urxvt.url-select.launcher:   /usr/bin/dwb
urxvt.url-select.underline: true
urxvt*termName: rxvt-256color
!urxvt*.transparent: true
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
!urxvt*.shading: 101


shoot*font:             xft:Tamzen10x20r:pixelsize=11
shoot*scrollBar:        false
shoot*scrollBar_right:  false
shoot*foreground:       #8B7B8B
shoot*background:       black
shoot*borderless:       1

